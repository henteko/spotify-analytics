{
  "name": "@henteko/spotify-analytics",
  "version": "1.2.0",
  "description": "Spotify Podcast Analytics CLI & Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henteko/spotify-analytics.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./exporters": {
      "import": "./dist/exporters/index.js",
      "require": "./dist/exporters/index.js",
      "types": "./dist/exporters/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/index.ts",
    "cli": "tsx src/cli/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "spotify",
    "podcast",
    "analytics",
    "cli",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "async-mutex": "^0.5.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
